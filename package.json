{
  "name": "stube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export PORT=3001 && next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "db:pull": "dotenv -e .env.local prisma db pull",
    "db:push": "dotenv -e .env.local prisma db push",
    "db:mig": "dotenv -e .env.local prisma migrate dev",
    "db:reset": "dotenv -e .env.local prisma migrate reset",
    "db:seed": "dotenv -e .env.local prisma db seed",
    "db:gen": "dotenv -e .env.local prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next": "16.0.0",
    "next-auth": "5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.18.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
